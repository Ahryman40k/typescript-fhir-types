{
  "name": "@imaware/ts-fhir-types",
  "version": "4.0.34",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run clean && npm run build:prod",
    "build": "tsc",
    "build:prod": "tsc --project ./tsconfig.prod.json",
    "test": "jest",
    "clean": "rimraf lib/*",
    "commitlint": "commitlint",
    "husky:install": "husky install",
    "lint": "run-s eslint:check prettier:check",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "eslint:check": "eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "prettier \"**/*.ts\" \"**/*.js\" -l",
    "prettier:fix": "prettier \"**/*.ts\" \"**/*.js\" --write",
    "mdlint": "markdownlint -c .markdownlint.jsonc -i CHANGELOG.md **.md",
    "mdlint:fix": "markdownlint -c .markdownlint.jsonc -i CHANGELOG.md -f **.md"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:imaware/typescript-fhir-types.git"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "codecov": "^3.8.0",
    "commitlint": "^15.0.0",
    "eslint": "^7.22.0",
    "husky": "^7.0.1",
    "jest": "^26.5.2",
    "lint-staged": "^12.0.2",
    "markdownlint-cli": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "fp-ts": "^2.8.3",
    "io-ts": "^2.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**.ts": [
      "yarn eslint:fix",
      "yarn prettier:fix"
    ],
    "src/**/*.ts": [
      "yarn eslint:fix",
      "yarn prettier:fix"
    ],
    "**.md": [
      "yarn mdlint:fix"
    ]
  }
}
